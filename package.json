{
  "name": "ccxt",
  "version": "1.7.111",
  "description": "A library for cryptocurrency trading and e-commerce with support for many bitcoin/ether/altcoin exchange markets and merchant APIs",
  "main": "./ccxt.js",
  "unpkg": "build/ccxt.browser.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccxt-dev/ccxt.git"
  },
  "scripts": {
    "build": "npm run export-exchanges && npm run vss && npm run mdrst && npm run transpile && npm run update-badges && npm run browserify",
    "test": "npm run build && node run-tests",
    "fasttest": "node run-tests --js --es6",
    "test-base": "mocha --reporter spec test/test_base.js",
    "export-exchanges": "node export-exchanges",
    "update-badges": "node update-badges",
    "mdrst": "pandoc --wrap=preserve --columns=10000 --from=markdown --to=rst --output=README.rst README.md",
    "transpile": "node transpile",
    "vss": "node vss",
    "browserify": "browserify --debug ./ccxt.browser.js > ./build/ccxt.browser.js"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "node-fetch": "2.0.0-alpha.8",
    "qs": "6.5.0"
  },
  "devDependencies": {
